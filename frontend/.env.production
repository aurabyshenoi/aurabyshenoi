# Production Environment Variables for Frontend

# API Configuration - Using Netlify Functions (same domain)
VITE_API_URL=https://aurabyshenoi.netlify.app
VITE_API_TIMEOUT=10000

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=your-cloud-name
VITE_CLOUDINARY_API_KEY=your-api-key



# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=true

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_PWA=false

# Security
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://aurabyshenoi.com,https://www.aurabyshenoi.com

# Image Optimization
VITE_IMAGE_QUALITY=80
VITE_IMAGE_FORMAT=auto
VITE_ENABLE_WEBP=true

# Cache Configuration
VITE_CACHE_MAX_AGE=31536000
VITE_ENABLE_SERVICE_WORKER=false